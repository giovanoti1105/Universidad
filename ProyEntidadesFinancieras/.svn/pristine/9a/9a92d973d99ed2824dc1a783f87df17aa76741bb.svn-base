package EntidaesFinancieras;
public class Nodo {
    private int dato;
    //auto referencia
    private Nodo enlace;
    // CONSTRUCTOR NODO SE INICIALIZAN LOS ATRIBUTOS DE LA CLASE POR DEFECTO
    public Nodo(){
        this.dato=0;
        this.enlace=null;
    }
    // constructor nodo inicializa el atributo dato de la clase
    public Nodo(int dato, Nodo enlace) {
        this.dato = dato;
        this.enlace = enlace;
    }

    // metodos Get and Set
    public int getDato() {
        return dato;
    }
    public void setDato(int dato) {
        this.dato = dato;
    }
    //obtener el valor de la referencia
    public Nodo getEnlace(){
        return enlace;
    }
    //se actualiza la referencia del nodo
    public void setEnlace(Nodo enlace){
        this.enlace=enlace;
    }
}
