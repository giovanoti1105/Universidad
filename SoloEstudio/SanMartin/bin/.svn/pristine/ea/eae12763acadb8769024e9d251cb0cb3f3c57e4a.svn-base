package primerSemestre;

import javax.swing.JOptionPane;

public class MaximoValor
{
	public static long elMayor(int x1, int x2)
	{
		long mayor = Math.max(x1, x2);
		return mayor;
	}

	public static void main(String[] args)
	{
		int n1=0;
		int n2=0;
		long mayor = 0;
		String cadena = JOptionPane.showInputDialog("Digite numero uno");
		n1 = Integer.parseInt(cadena);
		String cadenados = JOptionPane.showInputDialog("Digite numero dos");
		n2 = Integer.parseInt(cadenados);
		
		mayor = elMayor(n2, n2);
		JOptionPane.showMessageDialog(null, "El mayor es " + mayor);
		System.exit(0);
		
	}

}
