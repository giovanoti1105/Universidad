package primerSemestre;

public class Incremento
{
	public static void main(String[] args)
	{
		int n=1;
		System.out.println("n: " +n);
		System.out.println("++n: " + ++n);
		System.out.println("n++: " + n++);
		System.out.println("n: " + n);
		System.out.println("--n: " + --n);
		System.out.println("n--: " + n--);
		System.out.println("n: " + n);
	}
}
