package primerSemestre;

import java.util.Scanner;

public class NumeroEntre
{

	public NumeroEntre()
	{
	}

	public static void main(String[] args)
	{
		int numero = 0;
		Scanner objScanner = new Scanner(System.in);
		System.out.println("Ingrese numero");
		numero = objScanner.nextInt();

		switch (numero)
		{
		case 1:
		case 2:
		case 3:
			System.out.println("Numero entre 1 y 3");
			break;
		case 4:
		case 5:
			System.out.println("Numero entre 4 0 6");
			break;
		case 7:
			System.out.println("El numero es 7");
		default:
			System.out.println("Fuera del rango entre 1 y 7");
			break;
		}
	}

}
