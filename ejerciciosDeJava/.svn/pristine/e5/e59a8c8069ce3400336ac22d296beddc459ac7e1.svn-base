package EjerciciosProyectosInformaticos;

import EjerciciosProyectosInformaticos.tipos.Contacto;

public class Personal implements Contacto
{
	private String nombre;
	private String apellidos;
	private String telefono;
	private static int nInstancias;
	
	public Personal()
	{
		super();
		iniciaAtributos();
	}
	public Personal(String nombre, String apellidos, String telefono)
	{
		this.nombre=nombre;
		this.apellidos=apellidos;
		this.telefono=telefono;
		nInstancias++;
	}
/**
 * inicializa los atributos 
 */
	public void iniciaAtributos()
	{
	}
	/**
	 * @return el numero de instancias creadas
	 *  */
	public static int getNInstancias()
	{
		return nInstancias;
	}
}
