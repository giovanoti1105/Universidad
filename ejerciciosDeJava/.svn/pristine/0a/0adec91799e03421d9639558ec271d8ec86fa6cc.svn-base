package Varios.Guis;

import javax.swing.JFrame;
import javax.swing.JSlider;

public class Sliders extends JFrame
{
	public Sliders()
	{
		super("Ejemplo Sliders");
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		JSlider objSlider = new JSlider(JSlider.HORIZONTAL,0,30,5);
		objSlider.setMajorTickSpacing(10);
		objSlider.setMinorTickSpacing(1);
		objSlider.setPaintTicks(true);
		objSlider.setPaintLabels(true);
		add(objSlider);
	}
	public static void main(String[] args)
	{
		Sliders marco = new Sliders();
		marco.pack();
		marco.setVisible(true);
	}
}
