package EjerciciosProyectosInformaticos.tipos;

public class Ciudadano extends Persona
{
	// definicion de las variables de clase ciudadano
	private String poblacion;
	private String provincia;
	private int edad;

	// constructor de la clase
	public Ciudadano()
	{
		super();
		iniciaAtributos();
	}

	public Ciudadano(String nombre, String apellido, String telefono, int edad)
	{
	}

	@Override
	protected void iniciaAtributos()
	{
		setNombre("Remplazado por uno");
		setApellido("Remplazado por dos");
		setTelefono("Nuevo telefono");
		edad = 33;
		provincia="muy lejos";
		poblacion="Bastante Lejos";
		
		System.out.println(getNombre());
		System.out.println(getApellidos());
		System.out.println(getTelefono());
		System.out.println(getEdad());
		System.out.println(getProvincia());
		System.out.println(getPoblacion());
		
	}

	public String getPoblacion()
	{
		return poblacion;
	}

	public void setPoblacion(String poblacion)
	{
		this.poblacion = poblacion;
	}

	public String getProvincia()
	{
		return provincia;
	}

	public void setProvincia(String provincia)
	{
		this.provincia = provincia;
	}

	public int getEdad()
	{
		return edad;
	}

	public void setEdad(int edad)
	{
		this.edad = edad;
	}

}
