package Varios;

public class Arreglos {
	double[] arreglos = new double[5];
	// funcion que llena el arreglo y retorna un array lleno
	double[] LlenandoArreglo() {
		for (int i = 0; i < arreglos.length; i++) {
			arreglos[i] = Math.random();
			 System.out.println(arreglos[i]);
		}
		return arreglos;
	}
	
	// metodo principal que imprime los valores del arreglo
	public static void main(String[] args) {
		// creacion de objeto que permite el llamado de la funcion que almacena los valores
		Arreglos llamado = new Arreglos();
		llamado.LlenandoArreglo();
	}
}
